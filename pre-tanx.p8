-- main

function _init()
 cur_map = 2
 map_ofs = 16

 timer = 0
 // how many frames 
 // between 'inputs'
 // make this lower to speed it
 // up, at 1 you go same speed
 // as a shot (but turning hard)
 timer_rst = 3
 
 screen = in_splash
 
 cur_diff=1
 menu_line=1
 // this is a timer to make the
 // player wait before he can
 // shoot again
 shottimer_rst = 15
 // this is how long a shot lasts
 // for
 shot_dur = 105
 
 // over water, shot duration
 // goes down quickly
 water_shot = 11
 
 splash_timer=0
 menu_timer=0
 playing_track=false
 anim_timer=0
  
	setup_vars()
	setup_gfx()
	setup_shots()
	setup_bounce()

 scroll_start=1
 scroll_anim=0
 scrpos = 0
 spacers="                               "
 scroll_str=spacers .. 
  "another carefully planned high-quality "..
  "robsoft release (c) robsoft 2018 .... lol ...." ..
  "big thank you to adam miller for the lo-fi "..
  "logo, i bodged your better job mate and i "..
  "will do a better implementation next time! "..
  "heartfelt thanks to bridgs, mboffin & gruber "..
  "for pico-8 inspiration and assistance. cheers üê±"..
  spacers

	// finally, we start with the
	// splash
	cur_screen=in_splash
	
end


function _draw()
 if cur_screen==in_splash then
  draw_splash()
 elseif cur_screen==in_menu then
  draw_menu()
 else if cur_screen==in_game then
  draw_game()
 else
  draw_gameover()
 end  
end


function _update()
 if cur_screen==in_menu then
  update_menu()
 elseif cur_screen==in_splash then
  update_splash()
 elseif cur_screen==in_gameover then
  update_gameover()
 else  
  update_game()
 end
end

function update_game()
 // only check inputs if timer fires
 if timer<=0 then
  move_tank(p1,p2)
  move_tank(p2,p1)
  timer=timer_rst
 end
	timer-=1  

 // hold off 'reload'
 if p1.shottimer>0 then p1.shottimer-=1 end
 if p2.shottimer>0 then p2.shottimer-=1	end
 
	move_shots()
 check_hits()
 end
end

function update_splash()
 splash_timer+=1
 if btnp(4,0) or btnp(5,0) 
   or (splash_timer>150) then
  menu_timer=0
  cur_screen=in_menu
 end
end

function update_menu()
 if playing_track==false then
  music(0)
  playing_track=true
 end
 
 anim_timer+=1
 if anim_timer>=20 then
   anim_timer=0
 end
 
 if (btnp(2,0) or btnp(2,1)) and menu_line==2 then
  menu_line=1
  menu_timer=0
 end
 
 if (btnp(3,0) or btnp(3,1)) and menu_line==1 then
  menu_line=2
  menu_timer=0
 end
 
 if btnp(0,0) or btnp(0,1) then
  if menu_line==1 and cur_map>1 then
   cur_map-=1
  elseif menu_line==2 and cur_diff>1 then
   cur_diff-=1
  end
  menu_timer=0
 end
 
 if btnp(1,0) or btnp(1,1) then
  if menu_line==1 and cur_map<4 then
   cur_map+=1
  elseif menu_line==2 and cur_diff<2 then
   cur_diff+=1
  end
  menu_timer=0
 end
 
 if btnp(5,0) or btnp(5,1) then
  // wait for the buttons to be released
  repeat
  until btn(5,0)==false and btn(5,1)==false
    
  init_game()
  music(-1) 
  playing_track=false
  cur_screen=in_game
  return
 end
 
 scrpos-=1
 if scrpos<=-4 then
   scrpos=0
   scroll_start+=1
   if scroll_start>=#scroll_str-22 then
    scroll_start=1
    splash_timer=0
    cur_screen=in_splash
   end
 end  
   
 menu_timer+=1
end



function update_gameover()
  if playing_track==false then
  // need to find a way to clear
  // the ongoing sound before
  // playing the tune, this is
  // a mess right now
   music(-1)
   music(4)
   playing_track=true
  end
 
 if btnp(5,0) or btnp(5,1) then
  music(-1)
  playing_track=false
  cur_screen=in_menu
 end
end

function init_game()
	setup_players()
 // how far into the maps is
 // the map we want to use?
	map_ofs=(cur_map-1)*16
 if cur_diff==1 then
  shottimer_rst = 20
  shot_dur = 105
  timer_rst = 3
 else
  shottimer_rst = 45
  shot_dur = 80
  timer_rst = 4
 end 
end

